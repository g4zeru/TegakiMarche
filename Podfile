platform :ios, '12.0'
inhibit_all_warnings!
target 'TegakiMarche' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  pod 'RxCocoa', '~>5'
  pod 'RxSwift', '~>5'
  pod 'RxOptional'
  pod 'SnapKit', '~>5' 
  pod 'Firebase/Auth'
  pod 'Firebase/Firestore'
  pod 'GoogleSignIn'
  pod 'Instantiate'
  pod 'InstantiateStandard'

  target 'TegakiMarcheTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
